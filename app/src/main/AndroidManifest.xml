<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tictac_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tictac_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HelpActivity" />
        <activity android:name=".StatsActivity" />
        <activity android:name=".ThreeBoardActivity" />
        <activity android:name=".FiveBoardActivity" />
        <activity android:name=".ThreeBoardHumanActivity" />
        <activity android:name=".FiveBoardHumanActivity" />
        <activity android:name=".StatsMultiplayerActivity"></activity>
    </application>

</manifest>